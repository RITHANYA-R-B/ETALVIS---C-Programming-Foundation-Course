// Get a number and print the total number of two-digit odd numbers in the number using function (with a return value)
#include<stdio.h>
int disp_total_2digit_odd(int x);
int main(){
    int x;
    scanf("%d",&x);
    int n=disp_total_2digit_odd(x);
    printf("%d",n);
    return 0;
}
int disp_total_2digit_odd(int x){
    int n=0;
    for(;x>=10;x=x/10)
        if((x%100)%2==1 && x%100>=10)
            n++;
    return n;
}
