// Print whether the middle two digits of a 4-digit number is prime or not using for loop
#include<stdio.h>
int main(){
    int x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    x=x/10;
    x=x%100;
    for(int i=2;i<=x/2;i++){
        if(x%i==0)
            n++;
    }
    if(n==0&&x!=1&&x!=0)
    printf("Prime");
    else
    printf("Not Prime");
    return 0;
}
