// Get three numbers and print the LCM of those numbers using for loop
#include<stdio.h>
int main(){
    int x,y,z;
    printf("Enter Three Numbers: ");
    scanf("%d%d%d",&x,&y,&z);
    for(int i=1;;i++){
        if(x==0 || y==0 || z==0){
            printf("0");
            break;
        }
        if(i%x==0 && i%y==0 && i%z==0){
            printf("%d",i);
            break;
        }
    }
    return 0;
}
