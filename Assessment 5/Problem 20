// Print total number of single digit prime numbers using for loop
#include<stdio.h>
int main(){
    int n=0;
    for(int i=1;i<10;i++){
        int p=0;
    for(int j=2;j<=i/2;j++){
        if(i%j==0)
            p++;
    }
    if(i==1)
        p++;
    if(p==0)
        n++;
    }
    printf("%d",n);
    return 0;
}
