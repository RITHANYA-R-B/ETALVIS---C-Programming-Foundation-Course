// Print the total number of single-digit prime numbers in the number using for loop
#include<stdio.h>
int main(){
    int x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    for(;x>0;x=x/10){
        int p=0;
        for(int i=2;i<=(x%10)/2;i++){
            if((x%10)%i==0)
                p++;
        }
        if(p==0 && (x%10)>1)
        n++;
    }
    printf("%d",n);
    return 0;
}
