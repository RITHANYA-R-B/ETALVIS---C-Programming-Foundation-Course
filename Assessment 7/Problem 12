// Get a number and print the sum of all digts (using functions)
#include<stdio.h>
void disp_sum_all_digits(int x);
int main(){
    int x;
    scanf("%d",&x);
    disp_sum_all_digits(x);
    return 0;
}
void disp_sum_all_digits(int x){
    int sum=0;
    for(;x>0;x=x/10)
        sum=sum+x%10;
    printf("%d",sum);
}
