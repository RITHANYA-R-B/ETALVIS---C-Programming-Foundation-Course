// Get a number and print whether it is prime or not (using functions)
#include<stdio.h>
void check_prime(int x);
int main(){
    int x;
    scanf("%d",&x);
    check_prime(x);
    return 0;
}
void check_prime(int x){
    int p=0;
    for(int i=2;i<=x/2;i++)
        if(x%i==0){
            p=1;
            break;
        }
    if(p==0 && x>1)
        printf("Prime");
    else
        printf("Not Prime");
}
