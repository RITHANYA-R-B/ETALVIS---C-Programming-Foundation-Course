// Get a number and print the total number of digts in it (using functions)
#include<stdio.h>
void count_total_digits(int );
int main(){
    int x;
    scanf("%d",&x);
    count_total_digits(x);
    return 0;
}
void count_total_digits(int x){
    int n=0;
    for(;x>0;x=x/10)
        n++;
    printf("%d",n);
}
