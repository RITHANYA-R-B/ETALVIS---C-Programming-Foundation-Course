// Print two digit odd numbers whose sum of digits is 7 (using functions)
#include<stdio.h>
void disp_2digit_odd_sum7();
int main(){
    disp_2digit_odd_sum7();
    return 0;
}
void disp_2digit_odd_sum7(){
    for(int i=10;i<=99;i++)
        if(i%2==1 && i%10+i/10==7)
            printf("%d\n",i);
}
