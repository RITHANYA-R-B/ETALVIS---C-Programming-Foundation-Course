// Get a number and print whether it is prime or not.
#include<stdio.h>
int main()
{
    int x,n=0,i=2;
    printf("Enter Number: ");
    scanf("%d",&x);
    if(x>1){
    loop: if(i<=x/2){
            if(x%i==0)
            n=n+1;
        i=i+1;
        goto loop;
    }
    }
    if(n==0 && x>1)
    printf("Prime");
    else
    printf("Not Prime");
    return 0;
}
