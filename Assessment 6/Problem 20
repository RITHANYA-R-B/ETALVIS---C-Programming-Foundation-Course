// Print total number of single digit prime numbers using while loop
#include<stdio.h>
int main(){
    int i=1,n=0;
    while(i<10){
        int p=0,j=2;
    while(j<=i/2){
        if(i%j==0)
            p++;
        j++;
    }
    if(i==1)
        p++;
    if(p==0)
        n++;
    i++;
    }
    printf("%d",n);
    return 0;
}
