// Get three numbers and print the LCM of those numbers using while loop
#include<stdio.h>
int main(){
    int x,y,z,i=1;
    printf("Enter Three Numbers: ");
    scanf("%d%d%d",&x,&y,&z);
    while(i>0){
        if(x==0 || y==0 || z==0){
            printf("0");
            break;
        }
        if(i%x==0 && i%y==0 && i%z==0){
            printf("%d",i);
            break;
        }
        i++;
    }
    return 0;
}
