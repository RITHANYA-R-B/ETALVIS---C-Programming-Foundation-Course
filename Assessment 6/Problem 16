// Print whether prime or not using while loop
#include<stdio.h>
int main(){
    int i=2,x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    while(i<=x/2){
        if(x%i==0)
            n++;
        i++;
    }
    if(n==0 && x>1)
    printf("Prime");
    else
    printf("Not Prime");
    return 0;
}
