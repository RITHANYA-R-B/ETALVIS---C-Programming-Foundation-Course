// Print the total number of two-digit perfect square numbers in the number using while loop
#include<stdio.h>
int main(){
    int x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    while(x>0){
        if(x%100==16||x%100==25||x%100==36||x%100==49||x%100==64||x%100==81)
            n++;
        x=x/10;
    }
    printf("%d",n);
    return 0;
}
