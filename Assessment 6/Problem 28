// Get two numbers and print the LCM of those numbers using while loop
#include<stdio.h>
int main(){
    int x,y,i=1;
    printf("Enter Two Numbers: ");
    scanf("%d%d",&x,&y);
    while(i>0){
        if(x==0 || y==0){
            printf("0");
            break;
        }
        if(i%x==0 && i%y==0){
            printf("%d",i);
            break;
        }
        i++;
    }
    return 0;
}
