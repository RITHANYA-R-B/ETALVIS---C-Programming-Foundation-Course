// Print the total count of numbers which are less than 100000 and whose sum of digits is 14 using while loop
#include<stdio.h>
int main(){
    int n=0,i=0;
    while(i<100000){
        int sum=0,j=i;
        while(j>0){
        sum = sum + j%10;
        j=j/10;
        }
        if(sum==14)
            n++;
        i++;
    }
    printf("%d",n);
    return 0;
}
