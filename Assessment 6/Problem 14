// Interchange the first and the last digit of a number using while loop
#include<stdio.h>
int main(){
    int x,n,m,p=1;
    printf("Enter Number: ");
    scanf("%d",&x);
    m=x;
    while(m>9){
        p=p*10;
        m=m/10;
    }
    n=x/p+(x%10)*p+x%p-x%10;
    printf("%d",n);
    return 0;
}
