// Print the total number of single-digit prime numbers in the number using while loop
#include<stdio.h>
int main(){
    int x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    while(x>0){
        int p=0;
        int i=2;
        while(i<=(x%10)/2){
            if((x%10)%i==0)
                p++;
            i++;
        }
        if(p==0 && (x%10)>1)
            n++;
        x=x/10;
    }
    printf("%d",n);
    return 0;
}
