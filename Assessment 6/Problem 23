// Print the total number of single-digit perfect square numbers in the number using while loop
#include<stdio.h>
int main(){
    int x,n=0;
    printf("Enter Number: ");
    scanf("%d",&x);
    while(x>0){
        if(x%10==0 || x%10==1 || x%10==4 || x%10==9)
            n++;
        x=x/10;
    }
    printf("%d",n);
    return 0;
}
